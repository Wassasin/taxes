{
  "presets": ["react", "es2015", "stage-3"],
  "env": {
    "node": {
      "presets": ["react", "es2015", "stage-3", [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          },
          "modules": false
        }
      ]],
      "plugins": [
        ["transform-runtime", { "polyfill": false, "regenerator": true }],
        ["inline-json-import"],
        ["babel-plugin-transform-require-ignore", { "extensions": [".scss"] }]
      ]
    }
  }
}
